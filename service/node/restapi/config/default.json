{
  "argon2":
  {
    "memory_cost" : 65536,
    "parallelism" : 1,
    "salt_bytes" : 16,
    "time_cost" : 3
  },
  "locale":
  {
    "language": "en-US",
    "timezone": "America/New_York"
  },
  "log":
  {
    "dir": "/var/log/node",
    "access": "/var/log/node/access.log",
    "access_auth": "/var/log/node/authaccess.log",
    "error": "/var/log/node/error.log",
    "error_auth": "/var/log/node/autherror.log"
  },
  "mongodb":
  {
    "collection_auth_users": "auth_users",
    "database_auth": "admin",
    "database_default": "restapi",
    "host": "127.0.0.1",
    "password_file": "/home/node/etc/mongodb_password",
    "port": 27017,
    "user": "node"
  },
  "pidfile": "/var/run/node.pid",
  "redis":
  {
    "host": "127.0.0.1",
    "password_file": "/home/node/etc/redis_password",
    "port": 6379,
    "user": "node"
  },
  "restapi":
  {
    "access_token_secret_file": "/home/node/etc/access_token_secret",
    "password_file": "/home/node/etc/restapi_password",
    "refresh_token_secret_file": "/home/node/etc/refresh_token_secret",
    "user": "node"
  },
  "server":
  {
    "host": "127.0.0.1",
    "port" : 8889
  },
  "ssl_server":
  {
    "ca_file": "/etc/ssl/certs/ca-crt-supervisor.pem.pem",
    "client_cert_file": "/etc/ssl/localcerts/client-crt.pem",
    "client_key_file": "/etc/ssl/localcerts/client-key.pem",
    "host": "zeus",
    "port" : 4443
  }
}

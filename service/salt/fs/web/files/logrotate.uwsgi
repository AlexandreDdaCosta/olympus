/var/log/uwsgi/uwsgi.log {
    compress
    create 0644 uwsgi uwsgi
    dateext
    missingok
    notifempty
    postrotate
	/usr/sbin/service uwsgi restart 2>/dev/null
    rotate 10
    size 1024k
    endscript
}
/var/log/django/django.log {
    compress
    create 0644 uwsgi uwsgi
    dateext
    missingok
    notifempty
    postrotate
	/usr/sbin/service uwsgi restart 2>/dev/null
    rotate 10
    size 1024k
    endscript
}
